        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8733/Design_Time_Addresses/WcfServiceLibrary1/Videocard/" />
          </baseAddresses>
        </host>
      </service>
      
      <service name="WcfServiceLibrary1.PowerSupply">
